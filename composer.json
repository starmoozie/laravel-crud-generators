{
    "name": "starmoozie/laravel-crud-generators",
    "description": "Generate files for laravel projects",
    "keywords": [
        "generators",
        "model",
        "view",
        "request",
        "config"
    ],
    "homepage": "https://github.com/starmoozie/laravel-crud-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Starmoozie",
            "email": "starmoozie@gmail.com",
            "role": "Creator & Maintainer"
        }
    ],
    "require": {
        "starmoozie/laravel-crud": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Starmoozie\\Generators\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Starmoozie\\Generators\\GeneratorsServiceProvider"
            ]
        }
    }
}
